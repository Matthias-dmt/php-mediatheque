<section class="card m-3 p-0" >
    <div class="card-header p-3">
        <h5 class="card-title">Liste des documents les plus empruntés</h5>
    </div>
    <ul class="list-group list-group-flush">
    {% for document in documents %}
         <li class="list-group-item"><a href="#">{{ document.title }} : emprunté {{ document.docId }} fois</a></li>
    {% endfor %}
    </ul>
</section>